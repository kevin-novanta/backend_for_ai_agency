{
  "outreach_enabled": true,
  "pause": false,
  "client_name": "Outbound Accelerator",
  "interactive_mode_default": false,
  "start_time": "00:00",
  "end_time": "24:00",
  "days_allowed": ["Mon", "Tue", "Wed", "Thu", "Fri", "Sat"],
  "daily_limit": 200,
  "schedule_interval_minutes": 5,
  "per_inbox_limit": 40,
  "sender_pool": [
    "info@outbound-accelerator.com",
    "bryan@outbound-accelerator.com",
    "kevin@outbound-accelerator.com",
    "claude@outbound-accelerator.com",
    "jason@outbound-accelerator.com"
  ],
  "state_file": "workflows/outreach_sender/state/opener_state.json",
  "jitter_seconds_min": 30,
  "jitter_seconds_max": 90,

  "use_deliverability_filter": true,
  "allowed_deliverability_statuses": ["Safe"],

  "verification": {
    "enabled": true,
    "provider": "zerobounce",
    "cache_days": 90,
    "block_statuses": ["risky", "catch-all", "undeliverable", "unknown"]
  }
}